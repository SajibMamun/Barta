<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/left_nav"
    app:startDestination="@id/online">

    <fragment
        android:id="@+id/online"
        android:name="com.learnwithsajib.barta.Online"
        android:label="Online"
        tools:layout="@layout/fragment_online" >
        <action
            android:id="@+id/action_online_to_profile"
            app:destination="@id/profile" />
        <action
            android:id="@+id/action_online_to_notification"
            app:destination="@id/notification" />
        <action
            android:id="@+id/action_online_to_setting"
            app:destination="@id/setting" />
        <action
            android:id="@+id/action_online_to_useDetails"
            app:destination="@id/useDetails" />

    </fragment>
    <fragment
        android:id="@+id/notification"
        android:name="com.learnwithsajib.barta.Notification"
        android:label="Notification"
        tools:layout="@layout/fragment_notification" >

    </fragment>
    <fragment
        android:id="@+id/setting"
        android:name="com.learnwithsajib.barta.Setting"
        android:label="Setting"
        tools:layout="@layout/fragment_setting" >

    </fragment>

    <fragment
        android:id="@+id/profile"
        android:name="com.learnwithsajib.barta.Profile"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" >


    </fragment>
    <fragment
        android:id="@+id/useDetails"
        android:name="com.learnwithsajib.barta.UseDetails"
        android:label="fragment_use_details"
        tools:layout="@layout/fragment_use_details" />
</navigation>